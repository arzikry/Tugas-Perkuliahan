# Mendefinisikan fungsi kita sebagai seidel yang membutuhkan 2 argumen
# sebagai matriks A, dan matriks B
   
def seidel(a, x ,b):
    #Mencari panjang a(3)    
    n = len(a)                   
    # untuk loop selama 3 kali untuk menghitung x, y , z
    for j in range(0, n):        
        #variabel temp d untuk menyimpan b[j]
        d = b[j]                  
          
        #untuk menghitung masing-masing xi, yi, zi
        for i in range(0, n):     
            if(j != i):
                d-=a[j][i] * x[i]
        # memperbarui nilai solusi   
        x[j] = d / a[j][j]
    #mengembalikan solusi terbaru
    return x    
   
# int(input())input sebagai jumlah variabel yang akan diselesaikan             
n = 2                              
a = []                            
b = []        
# solusi awal tergantung pada n(di sini n=2)                   
x = [0, 0,]                        
a = [[2, 1],[1,3],]
b = [5,5]
print(x)
  
#loop dijalankan selama n kali tergantung pada n nilai kesalahan
for i in range(0, 25):            
    x = seidel(a, x, b)
    #cetak setiap kali solusi yang diperbarui
    print(x)  
