%
disp ('Andi Rachman Zikry')
disp ('Menghitung Sudut antar Vektor')
%
%Vektor
u = [ 4.0000    -5.0000    3.0000 ]
%
% Sudut Alpha
u_norm  = norm(u);
u_alpha = u (1)/u_norm;
theta   = acosd(u_alpha);
%
% Sudut Beta
u_beta = u(2)/u_norm;
beta   = acosd(u_beta);
%
% Sudut Gamma
u_gamma = u(3)/u_norm;
gamma   = acosd(u_gamma);
%
% Hasil Perhitungan
fprintf('Sudut Alpha  = %10.5g \n',theta)
fprintf('Sudut Beta   = %10.5g \n',beta)
fprintf('Sudut Gamma  = %10.5g \n',gamma)
